{
  "name": "a2a-agenticdirect-client",
  "version": "1.0.0",
  "description": "A2A AgenticDirect Client - JavaScript UI with @a2a-js/sdk",
  "type": "module",
  "scripts": {
    "build": "esbuild src/app.js --bundle --outfile=dist/bundle.js --format=esm",
    "dev": "esbuild src/app.js --bundle --outfile=dist/bundle.js --format=esm --watch --servedir=. --serve=8080",
    "serve": "http-server . -p 8080 --cors"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.3.0",
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.24.2"
  }
}
